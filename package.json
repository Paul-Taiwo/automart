{
  "name": "automart",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "dev": "nodemon ./src/app --exec babel-node",
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
    "build": "babel ./src -d ./build --source-maps",
    "start": "npm run db:migrate && node ./build/app",
    "db:migrate": "babel-node ./src/database/migrations/seeds",
    "pretest": "cross-env NODE_ENV=test npm run db:migrate",
    "test": "nyc cross-env NODE_ENV=test --reporter=html  mocha --require babel-register ./src/tests/* --timeout 10000 --no-deprecation --exit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build"
  },
  "author": "Paul Taiwo Adedokun",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "core-js": "^3.0.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "fancy-log": "^1.3.3",
    "generate-password": "^1.4.2",
    "googleapis": "^40.0.0",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "npm": "^6.9.0",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "regenerator-runtime": "^0.13.2",
    "swagger-ui-dist": "^4.1.3",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": "8.11.3"
  }
}
